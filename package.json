{
  "name": "nellow",
  "version": "0.0.0",
  "main": "build/server.js",
  "repository": "ssh://git@github.com/HAL-Future-Creation-Exhibition/nellow.git",
  "author": "konojunya <junya03dance@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node build/server.js",
    "predev": "yarn prebuild && yarn build:server",
    "dev": "yarn build:ts --mode=development --watch --devtool=inline-source-map & yarn build:server --watch & env NODE_ENV=staging nodemon build/server.js",
    "prebuild": "rm -rf public/js",
    "build": "yarn build:server && yarn build:ts --mode=production",
    "build:ts": "webpack -p",
    "build:server": "tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json -t verbose 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "d3-ease": "^1.0.5",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "gsap": "^2.0.2",
    "hoist-non-react-statics": "^3.1.0",
    "method-override": "^3.0.0",
    "nodemon": "^1.18.5",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-move": "^2.8.0",
    "react-router-dom": "^4.3.1",
    "react-swipe": "^6.0.3",
    "swipe-js-iso": "^2.1.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/method-override": "^0.0.31",
    "@types/node": "^10.12.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "clean-webpack-plugin": "^0.1.19",
    "prettier": "^1.14.3",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  }
}
